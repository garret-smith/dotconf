#!/usr/bin/env sh

xrandr --output HDMI-1 --right-of eDP-1 --auto --rotate left
xrandr --output DP-2-2 --left-of eDP-1 --auto
#xrandr --output eDP-1 --gamma 1:.95:1 --brightness 1.0
xrandr --output eDP-1 --gamma 1.2:1.3:1.5 --brightness 1.0

setxkbmap -layout us

#xmodmap -e "pointer = 1 2 3"

xscreensaver &

